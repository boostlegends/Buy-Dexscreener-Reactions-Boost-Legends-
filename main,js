// main.js
const reactions = [
  { type: 'Fire', emoji: 'ðŸ”¥', description: 'Spark interest and excitement around your token.' },
  { type: 'Rocket', emoji: 'ðŸš€', description: 'Highlight tokens with explosive growth potential.' },
  { type: 'Poop', emoji: 'ðŸ’©', description: 'Add a touch of humor or indicate skepticism.' },
  { type: 'Red Flag', emoji: 'ðŸš©', description: 'Warn traders of potential risks in a timely manner.' }
];

function getReactions() {
  return reactions.map(reaction => `${reaction.type} ${reaction.emoji}: ${reaction.description}`).join('\n');
}

module.exports = { getReactions };

